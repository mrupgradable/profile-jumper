{
  "manifest_version": 2,
  "name": "Profile Jumper! Jump easily between profiles!",
  "short_name": "Profile Jumper!",
  "description": "Extension to help you jump to any of your online profiles. Can be social or other. Configure instantly. Open source and free!",
  "version": "0.0.8",
  "author": "Mr Upgradable",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "38": "images/icon38.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png",
    "192": "images/icon192.png"
  },
  "browser_action": {
    "default_icon": "images/icon192.png",
    "default_popup": "index.html#popup"
  },
  "options_page": "index.html#settings",
  "permissions": [
    "storage"
  ]
}
